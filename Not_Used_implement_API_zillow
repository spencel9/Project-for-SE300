#here is where I will implement the zillow API

import requests

url = "http://www.zillow.com/webservice/GetSearchResults.htm"
params = {
      "zws-id": "YOUR_ZILLOW_API_KEY", #this will be implemented once I am given a key
      "address": "1600 Pennsylvania Ave NW".
      "citystatezip": "Washinton, DC 20500"
}

response = requests.get(url, params=params)
