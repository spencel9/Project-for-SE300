import folium

# Create a map object with an initial location (latitude and longitude)
m = folium.Map(location=[29.1917, -81.0501], zoom_start=10)  # ERAU coordinates

# Add markers to the map
folium.Marker([29.1917, -81.0501], tooltip='ERAU').add_to(m)

# Save the map to an HTML file
m.save('basic_map3.html')

# Display the map in your Jupyter Notebook
m
